   "description": "Simple Ecommerce",
   "main": "index.js",
   "license": "UNLICENCED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
      "start": "node ./dist/shared/infra/http/server.js",
      "dev": "NODE_ENV=test ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
      "lint": "eslint --fix",
      "typeorm": "NODE_ENV=test ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "test": "NODE_ENV=test jest --runInBand --detectOpenHandles"
   },
   "dependencies": {
      "cors": "^2.8.5",
