      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.2",
      "swagger-ui-express": "^4.2.0",
      "tsyringe": "^4.6.0",
      "typeorm": "0.2.41",
      "uuid": "^8.3.2",
      "validator": "^13.7.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.16.0",
      "@babel/core": "^7.16.0",
      "@babel/node": "^7.16.0",
      "@babel/plugin-proposal-class-properties": "^7.16.0",
